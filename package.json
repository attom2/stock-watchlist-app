{
	"name": "tasty-works-andy-tom",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev -- --open",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync && npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@smui-extra/autocomplete": "^6.0.0-beta.16",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/circular-progress": "^6.0.0-beta.16",
		"@smui/common": "^6.0.0-beta.16",
		"@smui/data-table": "^6.0.0-beta.16",
		"@smui/icon-button": "^6.0.0-beta.16",
		"@smui/linear-progress": "^6.0.0-beta.16",
		"@smui/list": "^6.0.0-beta.16",
		"@smui/textfield": "^6.0.0-beta.16",
		"@smui/tooltip": "^6.0.0-beta.16",
		"@smui/top-app-bar": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"autoprefixer": "^10.4.7",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.3",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"axios": "^0.27.2",
		"cookie": "^0.4.1"
	}
